{
  "expo": {
    "name": "Travel Time",
    "slug": "travel-time",
    "description": "Travel app for your travel partner",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/app/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/app/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "traveltime.com",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {}
  }
}
